@page
@model IndexModel
@using Team_3_mueseum;
@{
    ViewData["Title"] = "Home page";
}

@inject UserService user
@{
    var users = user.GetUser();
}

<h2>User List</h2>
<table class="User_table">
    <tr>
        <th>UserID</th>
        <th>Email</th>
        <th>Password</th>
    </tr>
    @foreach (var u in users)
    {
    <tr>
        <td>@u.UserID</td>
        <td>@u.Email</td>
        <td>@u.Password</td>
    </tr>
    }
</table>

<br />
<br />

<h2 style="font-size: 24px; font-weight: bold;">Membership Data</h2>

<div class="membership-button-container">
    <a href="/Membership">
        <button class="view-memberships-button">View Memberships</button>
    </a>
</div>